# Expo Configuration
EXPO_PROJECT_ID=your_expo_project_id
EXPO_PUBLIC_EXPO_PROJECT_ID=your_expo_project_id

# App Configuration
EXPO_PUBLIC_APP_NAME=KP5 Academy
EXPO_PUBLIC_APP_URL=https://your-app-url.com
EXPO_PUBLIC_API_URL=https://your-api-url.com

# Real-time Communication
EXPO_PUBLIC_WEBSOCKET_URL=wss://your-api-url.com

# File Upload Configuration
EXPO_PUBLIC_FILE_UPLOAD_URL=https://your-api-url.com/api/upload
EXPO_PUBLIC_CLOUD_STORAGE_URL=https://your-storage-bucket.s3.amazonaws.com

# Google Maps API (for location features)
EXPO_PUBLIC_GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# Analytics
EXPO_PUBLIC_GA_TRACKING_ID=G-XXXXXXXXXX

# Feature Flags
EXPO_PUBLIC_ENABLE_PUSH_NOTIFICATIONS=true
EXPO_PUBLIC_ENABLE_OFFLINE_MODE=true
EXPO_PUBLIC_ENABLE_LOCATION_FEATURES=true
EXPO_PUBLIC_ENABLE_CAMERA_FEATURES=true
EXPO_PUBLIC_ENABLE_REAL_TIME=true

# Development Settings
NODE_ENV=development
EXPO_PUBLIC_DEBUG_MODE=true

# Expo Updates
EXPO_PUBLIC_UPDATES_ENABLED=true
EXPO_PUBLIC_UPDATES_CHECK_ON_LAUNCH=ALWAYS
EXPO_PUBLIC_UPDATES_FALLBACK_TO_CACHE_TIMEOUT=0

# Expo Notifications
EXPO_PUBLIC_NOTIFICATION_ICON=./assets/notification-icon.png 